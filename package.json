{
  "name": "alf-cdk-app-pipeline",
  "version": "0.1.0",
  "main": "app.js",
  "types": "app.d.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "no-changes": "(git log --oneline -1 | grep -q \"chore(release):\") && echo \"No changes to release.\"",
    "bump": "yarn run --silent no-changes || standard-version",
    "release": "yarn run --silent no-changes || (yarn run bump && git push --follow-tags origin master)"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudfront": "1.73.0",
    "@aws-cdk/aws-codebuild": "1.73.0",
    "@aws-cdk/aws-codecommit": "1.73.0",
    "@aws-cdk/aws-codedeploy": "1.73.0",
    "@aws-cdk/aws-codepipeline": "1.73.0",
    "@aws-cdk/aws-codepipeline-actions": "1.73.0",
    "@aws-cdk/aws-ec2": "1.73.0",
    "@aws-cdk/aws-elasticloadbalancingv2-targets": "1.73.0",
    "@aws-cdk/aws-iam": "1.73.0",
    "@aws-cdk/aws-lambda": "1.73.0",
    "@aws-cdk/aws-lambda-event-sources": "1.73.0",
    "@aws-cdk/aws-route53": "1.73.0",
    "@aws-cdk/aws-route53-targets": "1.73.0",
    "@aws-cdk/aws-s3": "1.73.0",
    "@aws-cdk/aws-s3-deployment": "1.73.0",
    "@aws-cdk/aws-sqs": "1.73.0",
    "@aws-cdk/aws-ssm": "1.73.0",
    "@aws-cdk/aws-stepfunctions-tasks": "1.73.0",
    "@aws-cdk/core": "1.73.0",
    "@aws-cdk/pipelines": "1.73.0",
    "@aws-solutions-constructs/aws-dynamodb-stream-lambda": "1.73.0",
    "@aws-solutions-constructs/aws-lambda-dynamodb": "1.73.0",
    "@aws-solutions-constructs/aws-sqs-lambda": "1.73.0",
    "@mobileposse/auto-delete-bucket": "1.55.0",
    "@types/node": "^14.14.7",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "typescript": "^4.0.5",
    "prettier": "^2.1.2",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "standard-version": "^9.0.0"
  }
}
